# 应用环境变量示例
# 复制此文件为 .env.development 或 .env.production 并根据需要修改

# API配置
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# 高德地图配置
VITE_AMAP_API_KEY=your_amap_api_key_here
VITE_AMAP_SECURITY_KEY=your_amap_security_key_here
VITE_AMAP_VERSION=2.0

# 应用配置
VITE_APP_TITLE=铁路客运智能分析与可视化系统
VITE_APP_DESCRIPTION=基于北欧美学的成渝地区铁路客运数据分析平台
VITE_APP_VERSION=1.0.0

# 功能开关
VITE_ENABLE_MOCK_API=false
VITE_ENABLE_ANALYTICS=true